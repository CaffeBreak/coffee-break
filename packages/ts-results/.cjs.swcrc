{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": false,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "target": "esnext",
    "loose": false,
    "externalHelpers": true,
    "keepClassNames": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "minify": false,
  "module": {
    "type": "commonjs"
  }
}
