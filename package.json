{
  "name": "coffee-break",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "pnpm lint -- --fix",
    "format": "turbo run format",
    "gen": "swc plopfile.ts -o plopfile.js && plop",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "@turbo/gen": "^1.10.15",
    "@types/node": "^18.18.5",
    "camelcase": "^7.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "plop": "^3.1.2",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.6.11",
  "pnpm": {
    "overrides": {
      "@nestjs/core@<9.0.5": ">=9.0.5",
      "webpack@>=5.0.0 <5.76.0": ">=5.76.0"
    }
  }
}
