{
  "name": "jinro",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.3.1",
  "scripts": {
    "dev": "WATCHPACK_POLLING=true run-z build:server watch:server,watch:next,watch:aspida,watch:frourio,watch:prisma",
    "watch:aspida": "pnpm --filter server watch:aspida",
    "watch:frourio": "pnpm --filter server watch:frourio",
    "watch:prisma": "pnpm build:prisma --watch",
    "watch:next": "node-dev --ignore=.next --notify=false --respawn built/server.js",
    "watch:server": "pnpm build:server -w",
    "build": "run-z build:server,build:next,build:aspida,build:frourio,build:prisma",
    "build:aspida": "pnpm --filter server build:aspida",
    "build:frourio": "pnpm --filter server build:frourio",
    "build:prisma": "prisma generate",
    "build:next": "next build",
    "build:server": "swc server.ts src/server/\\$server.ts src/server/api -d built -D",
    "migrate": "prisma migrate deploy",
    "start": "NODE_ENV=production node built/server.js",
    "migrateandstart": "run-z migrate start",
    "lint": "next lint",
    "format": "prettier src/**/*.{ts,tsx,json,css} --write",
    "typecheck": "tsc --noEmit",
    "clean-all": "run-z clean:module,clean:built,clean:server",
    "clean:module": "rm -rf node_modules",
    "clean:built": "rm -rf built .next",
    "clean:server": "pnpm --filter server clean",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aspida/fetch": "^1.12.0",
    "@fastify/cors": "^8.2.1",
    "@prisma/client": "^4.13.0",
    "fast-json-stringify": "^5.7.0",
    "fastify": "^4.17.0",
    "jotai": "^2.1.0",
    "next": "^13.4.1",
    "pino-pretty": "^10.0.0",
    "prisma": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "run-z": "^1.11.0",
    "socket.io": "^4.6.1",
    "velona": "^0.7.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/addon-mdx-gfm": "^7.0.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@types/node": "^18.16.5",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "camelcase": "^7.0.1",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "hygen": "^6.2.11",
    "node-dev": "^8.0.0",
    "postcss": "^8.4.23",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "storybook": "^7.0.9",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma"
  }
}